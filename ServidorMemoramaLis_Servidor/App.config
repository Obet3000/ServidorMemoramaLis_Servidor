<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<system.serviceModel>
		<services>
			<service name="ServidorMemoramaLis_Servidor.AutentificacionServicio">
				<endpoint address="http://localhost:8080/AutentificacionServicio"
                          binding="wsDualHttpBinding"
                          contract="ServidorMemoramaLis.Contracts.IAutentificacionServicio" />
			</service>

		</services>
		<behaviors>
			<serviceBehaviors>
				<behavior>
					<serviceMetadata httpGetEnabled="true" httpGetUrl="http://localhost:8080/AutentificacionServicioMetadata" />
					<serviceDebug includeExceptionDetailInFaults="false" />
				</behavior>
			</serviceBehaviors>
		</behaviors>
		<serviceHostingEnvironment multipleSiteBindingsEnabled="true" />
	</system.serviceModel>
</configuration>